# 애플리케이션 설정
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# 브라우저 설정
HEADLESS_BROWSER=false

# Redis 설정
REDIS_HOST=localhost
REDIS_PORT=6379

# 알림 설정
ALERTS_ENABLED=true
ALERTS_EMAIL_ENABLED=false
ALERTS_SLACK_ENABLED=false
ALERTS_SMS_ENABLED=false
ALERTS_ERROR_RATE_THRESHOLD=0.1
ALERTS_RESPONSE_TIME_THRESHOLD=5000
